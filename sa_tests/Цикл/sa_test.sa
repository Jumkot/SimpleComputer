00 = +0001 ; (Переменная А)
01 = +000a ; (Переменная В)
02 LOAD 01 ; (Загрузка B в аккумулятор)
03 SUB 00 ; (Отнять A)
04 STORE 05 ; (Выгрузка из аккумулятора)
06 WRITE 05 ; (Вывод выгруженного значения (B))
07 JZ 61 ; (Переход на 61, если 0)
08 JUMP 03 ; (Переход на 03)
61 HALT 00 ; (Останов)